#!/bin/bash

# Peptide Optimization 服务管理快捷脚本
# 在项目根目录可直接使用 ./service 命令

SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
SERVICE_SCRIPT="$SCRIPT_DIR/cicd/scripts/start_peptide_service.sh"

if [ -f "$SERVICE_SCRIPT" ]; then
    exec "$SERVICE_SCRIPT" "$@"
else
    echo "错误: 找不到服务脚本 $SERVICE_SCRIPT"
    exit 1
fi
